<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.modesteam.pardal.CompareFragment"
    android:id="@+id/compareXml">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Title"
        android:id="@+id/titleTextView"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="81dp"
        android:textColor="#ff000000"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/percentageTotalTicketsTextView"
        android:textColor="#ffff0000"
        android:layout_below="@+id/percentageTotalTicketsTitle"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Porcentagem de Velocidade Máx. Registrada"
        android:id="@+id/percentageMaximumTitle"
        android:textColor="#ff000000"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/percentageTotalTicketsTextView"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Porcentagem de Total de Multas"
        android:id="@+id/percentageTotalTicketsTitle"
        android:textColor="#ff000000"
        android:singleLine="false"
        android:layout_below="@+id/relativeLayout2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/percentageMaximumTextView"
        android:textColor="#ffff0000"
        android:layout_below="@+id/percentageMaximumTitle"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="335dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Estas Porcentagens são referentes a diferença do maior valor comparado com o menor valor."
        android:id="@+id/textView10"
        android:textColor="#ff000000"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="186dp"
        android:layout_height="250dp"
        android:id="@+id/relativeLayout2"
        android:layout_below="@+id/titleTextView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Velocidade Máx. Registrada"
            android:id="@+id/maximum1TextView"
            android:layout_marginTop="24dp"
            android:textColor="#ff4240ff"
            android:layout_below="@+id/totalTicketsValue1TextView"
            android:layout_alignRight="@+id/average1TextView"
            android:layout_alignEnd="@+id/average1TextView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Média do Limite Excedido"
            android:id="@+id/average1TextView"
            android:layout_marginTop="21dp"
            android:textColor="#ff4240ff"
            android:layout_below="@+id/itemName1textView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total de Multas"
            android:id="@+id/totalTickets1TextView"
            android:layout_marginTop="21dp"
            android:textColor="#ff4240ff"
            android:layout_below="@+id/averageValue1TextView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/averageValue1TextView"
            android:layout_below="@+id/average1TextView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/totalTicketsValue1TextView"
            android:layout_below="@+id/totalTickets1TextView"
            android:layout_alignLeft="@+id/averageValue1TextView"
            android:layout_alignStart="@+id/averageValue1TextView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/maximumValue1TextView"
            android:layout_below="@+id/maximum1TextView"
            android:layout_alignLeft="@+id/totalTicketsValue1TextView"
            android:layout_alignStart="@+id/totalTicketsValue1TextView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Large Text"
            android:id="@+id/itemName1textView"
            android:textColor="#ff4240ff"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="190dp"
        android:layout_height="200dp"
        android:id="@+id/relativeLayout"
        android:layout_alignTop="@+id/relativeLayout2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/relativeLayout2"
        android:layout_toRightOf="@+id/relativeLayout2"
        android:layout_above="@+id/percentageTotalTicketsTitle">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Média do Limite Excedido"
            android:id="@+id/average2TextView"
            android:layout_marginTop="21dp"
            android:textColor="#ffaeb32e"
            android:layout_below="@+id/itemName2textView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Total de Multas"
            android:id="@+id/totalTickets2TextView"
            android:textColor="#ffaeb32e"
            android:layout_below="@+id/averageValue2TextView"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="22dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Velocidade Máx. Registrada"
            android:id="@+id/maximum2TextView"
            android:layout_marginTop="21dp"
            android:textColor="#ffaeb32e"
            android:layout_below="@+id/totalTicketsValue2TextView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/averageValue2TextView"
            android:layout_below="@+id/average2TextView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/totalTicketsValue2TextView"
            android:layout_below="@+id/totalTickets2TextView"
            android:layout_alignLeft="@+id/averageValue2TextView"
            android:layout_alignStart="@+id/averageValue2TextView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/maximumValue2TextView"
            android:layout_below="@+id/maximum2TextView"
            android:layout_alignLeft="@+id/totalTicketsValue2TextView"
            android:layout_alignStart="@+id/totalTicketsValue2TextView" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Large Text"
            android:id="@+id/itemName2textView"
            android:textColor="#ffaeb32e"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />
    </RelativeLayout>

</RelativeLayout>
