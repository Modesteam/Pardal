<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.modesteam.pardal.TypeRankFragment">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/type_rank">
    <ImageButton
        android:layout_width="114dp"
        android:layout_height="95dp"
        android:id="@+id/button_totalTickets"
        android:layout_gravity="left|center_vertical"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button_maximumVelocity"
        android:layout_toStartOf="@+id/button_maximumVelocity"
        android:layout_marginBottom="130dp" />

    <ImageButton
        android:layout_width="107dp"
        android:layout_height="89dp"
        android:id="@+id/button_maximumVelocity"
        android:layout_gravity="center"
        android:layout_alignBottom="@+id/button_totalTickets"
        android:layout_alignLeft="@+id/button_averageExceded"
        android:layout_alignStart="@+id/button_averageExceded" />

    <ImageButton
        android:layout_width="112dp"
        android:layout_height="93dp"
        android:id="@+id/button_averageExceded"
        android:layout_gravity="right|center_vertical"
        android:layout_above="@+id/button_totalTickets"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="52dp" />
    </RelativeLayout>
</FrameLayout>
